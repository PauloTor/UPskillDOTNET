@model IEnumerable<ParqueWEB.Models.Lugar>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fila)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sector)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Preço)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Parque)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fila)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sector)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Preço)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Parque.ParqueID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LugarID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LugarID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LugarID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
